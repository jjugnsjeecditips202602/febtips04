<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <target>System.out</target>
        <encoder>
            <pattern>%d{yyyy/MM/dd HH:mm:ss} %marker %msg%n</pattern>
        </encoder>
    </appender>
</configuration>